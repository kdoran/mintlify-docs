{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Billiam Docs",
  "colors": {
    "primary": "#000000",
    "light": "#07C983",
    "dark": "#000000"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "High Level Planning",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "Etcetera",
            "pages": [
              "etc/thesis"
            ]
          }
        ]
      },
      {
        "tab": "Tech Stack",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Hey Billiam",
          "href": "https://heybilliam.com",
          "icon": "newspaper"
        }
      ]
